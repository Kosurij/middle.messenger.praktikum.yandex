<form class="overlay" id="dialog-form">
  <div class="dialog">
    <h3 class="dialog__title">
      {{ title }}
    </h3>
    <div class="dialog__content {{#if contentFullWidth}} dialog__content-fullWidth {{/if}}">
      {{{ content }}}
    </div>
    <div class="dialog__actions">
      {{{ cancel }}}
      {{{ submit }}}
    </div>
  </div>
</form>
